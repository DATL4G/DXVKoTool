CREATE TABLE SteamGame(
    appId INTEGER NOT NULL,
    cacheName TEXT NOT NULL,
    repoItem TEXT,
    PRIMARY KEY (appId, cacheName)
);

selectAll:
SELECT * FROM SteamGame;

insert:
INSERT OR REPLACE INTO SteamGame(appId, cacheName, repoItem)
VALUES (?, ?, ?);

insertObject:
INSERT OR REPLACE INTO SteamGame(appId, cacheName, repoItem)
VALUES ?;
